<table class"table">
  <h1>Users</h1>
  <tread>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>

  <% @users.each do |user| %>
  <tbody>
    <tr>
      <td>
        <div><%= attachment_image_tag user, :profile_image, :fill, 50, 50, format:'jpeg', class: "pull-left profile-img" %></div>
      </td>
      <td>
        <%= user.name %>
      </td>
      <td>
        <%= link_to "show", user_path(user.id), class: "user_#{user.id}" %>
      </td>
    </tr>
  </tbody>
  <% end %>
</table>
<%= render 'books/form', book: @book %>